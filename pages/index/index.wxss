/* logo section */
.logo {
  margin: 20rpx;
  display: flex;
  justify-content: space-around;
}
.foreground {
  position: absolute;
  width: 200rpx;
  height: 200rpx;
}
.background {
  box-sizing: border-box;
  padding: 40rpx 40rpx 0;
  padding-bottom: 100rpx;
  width: 200rpx;
  height: 200rpx;
  background-color: bisque;
  background-clip: content-box;
}
